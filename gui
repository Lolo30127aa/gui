local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ps99Script by Midnight#9066", colors)

local Tab = Window:NewTab("üêæPetDupingüîÑ")
local mainSection = Tab:NewSection("PetDuping")

local Tab = Window:NewTab("üß†SmartProtectionV1")
local SmartprotectionSection = Tab:NewSection("Smartprotection")

local Tab = Window:NewTab(" üöúAutofarmV1 üöú")
local AutofarmV1Section = Tab:NewSection("Autofarm")

local Tab = Window:NewTab(" üí∞PaidKey üí∞")
local PaidKeySection = Tab:NewSection("PaidKey")

local Tab = Window:NewTab("NukeV1")
local nukeSection = Tab:NewSection("üí£Nukeüí£")

local Tab = Window:NewTab("GemsAutofarm")
local GemsSection = Tab:NewSection("GemsAutofarm")



local notificationGui = Instance.new("ScreenGui")
notificationGui.Name = "NotificationGui"
notificationGui.Parent = game.CoreGui



nukeSection:NewButton("Nuke", "Nuke the whole server", function()
    -- Create a notification
    local notification = Instance.new("ScreenGui")
    notification.Name = "NukeNotification"
    notification.Parent = game.StarterGui

    local frame = Instance.new("Frame")
    frame.BackgroundColor3 = Color3.new(0, 0, 0) 
    frame.Size = UDim2.new(0, 300, 0, 30)
    frame.Position = UDim2.new(0, 0, 0, 30)
    frame.Parent = notification

    local textLabel = Instance.new("TextLabel")
    textLabel.Text = "Nuking in progress..."
    textLabel.TextColor3 = Color3.new(1, 1, 1) 
    textLabel.Font = Enum.Font.SourceSans
    textLabel.FontSize = Enum.FontSize.Size24
    textLabel.Parent = frame

 
    wait(3)
    notification:Destroy()


    local player = game.Players.LocalPlayer
    local placeId = game.PlaceId
    player:RejoinGameState(placeId, "AnotherServer")
end)




local DraggableFrame = Library.DraggableFrame("DraggableFrame", {
    BackgroundColor3 = colors.Background,
    Size = UDim2.new(0.5, 0, 0.5, 0),
    Position = UDim2.new(0.25, 0, 0.25, 0),
})

local MinusButton = DraggableFrame:NewButton("-", "Minimize the GUI", function()
    
    DraggableFrame:TweenPosition(UDim2.new(1, -50, 1, -50), "Out", "Quad", 0.5)
end)
MinusButton.Parent = DraggableFrame
MinusButton.Position = UDim2.new(1, 0, 0, 0)
DraggableFrame.Parent = game.CoreGui



